q[n+9, m+13] <- "dispHO+KO"
q[n+10, m+14] <- "dispMO+KO"
q[n+3, m+11] <- "dispHK+MK"
q[n+5, m+13] <- "dispHK+OK"
q[n+6, m+14] <- "dispMK+OK"
q[n+14, m+15] <- "dispMH+OH+KH" # 234 -> 1234
